{
  "name": "mymlg",
  "displayName": "MyMlg",
  "description": "A very simple language",
  "version": "0.0.1",
  "engines": {
      "vscode": "^1.54.0"
  },
  "categories": [
      "Programming Languages"
  ],
  "contributes": {
      "languages": [{
          "id": "coq_plugin",
          "extensions": [".mlg"],
          "configuration": "./languages/language-configuration.json"
      }
      ],
      "grammars": [{
          "language": "coq_plugin",
          "scopeName": "source.mlg",
          "path": "./syntaxes/coq_plugin.json"
      }
    ]
  }
  
}